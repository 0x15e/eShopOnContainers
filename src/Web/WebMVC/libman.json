{
  "version": "1.0",
  "defaultProvider": "cdnjs",
  "libraries": [
    {
      "provider": "filesystem",
      "library": "wwwroot/js_modules/jquery-validation/dist/",
      "destination": "wwwroot/lib/jquery-validate/",
      "files": [
        "jquery.validate.min.js"
      ]
    },
    {
      "provider": "filesystem",
      "library": "wwwroot/js_modules/jquery-validation-unobtrusive/dist/",
      "destination": "wwwroot/lib/jquery-validation-unobtrusive/",
      "files": [
        "jquery.validate.unobtrusive.min.js"
      ]
    },
    {
      "provider": "filesystem",
      "library": "wwwroot/js_modules/jquery/dist/",
      "destination": "wwwroot/lib/jquery/",
      "files": [
        "jquery.slim.min.js",
        "jquery.slim.min.map"
      ]
    },
    {
      "provider": "filesystem",
      "library": "wwwroot/js_modules/bootstrap/",
      "destination": "wwwroot/lib/bootstrap/",
      "files": [
        "dist/css/bootstrap.css",
        "dist/css/bootstrap.css.map",
        "dist/css/bootstrap.min.css",
        "dist/css/bootstrap.min.css.map",
        "dist/js/bootstrap.js",
        "dist/js/bootstrap.min.js"
      ]
    },
    {
      "provider": "filesystem",
      "library": "wwwroot/js_modules/toastr/build/",
      "destination": "wwwroot/lib/toastr/",
      "files": [
        "toastr.css",
        "toastr.js.map",
        "toastr.min.css",
        "toastr.min.js"
      ]
    },
    {
      "provider": "filesystem",
      "library": "wwwroot/js_modules/@aspnet/signalr/dist/browser",
      "destination": "wwwroot/lib/@aspnet/signalr/",
      "files": [
        "signalr.js",
        "signalr.js.map",
        "signalr.min.js",
        "signalr.min.js.map"
      ]
    }
  ]
}